---
import { fetchProgramas, type Programa } from "../../utils/fetchProgramas";
import Container from "@/components/containers/Container.astro";
import Layout from "@/layouts/Layout.astro";
import ProgramasHero from "@/components/Programas/Hero";

const programas: Programa[] = await fetchProgramas();
---

<Layout>
  <Container class="mt-40">
    <ProgramasHero client:load programas={programas} />
  </Container>
</Layout>
