---
import { fetchProgramas } from "@/utils/fetchProgramas";
import Container from "../containers/Container.astro";
import ProgramacionCarousel from "./ProgramacionCarousel";

const programas = await fetchProgramas();
---

<Container class="flex flex-col w-full">
  <h2 class="font-inter text-white font-semibold text-xl lg:text-2xl">
    Nuestros programas
  </h2>
  <ProgramacionCarousel programas={programas} client:only="react" />
</Container>
